{"payload":{"meta":{"Graph":{"@Type":"81297157","@Value":{"Guid":"(Guid)62ac5fa6-a7e8-4e41-a0d0-c62675174d1f","ParentGuid":"(Guid)00000000-0000-0000-0000-000000000000","Name":"(string)FB_TimeBasedClockGenerator","Properties":{"@Type":"2c41fa04:IDictionary","@Value":[{"@Key":"(Guid)24568a24-c491-472c-a21f-ee5d33859fab","@Value":{"@Type":"24568a24","@Value":{"MemoryReserveForOnlineChange":"(int)0","ExcludeFromBuild":"(bool)False","External":"(bool)False","EnableSystemCall":"(bool)False","CompilerDefines":"(string)","LinkAlways":"(bool)False","Undefines":{"@Type":"[string]","@Value":[]}}}},{"@Key":"(Guid)829a18f2-c514-4f6e-9634-1df173429203","@Value":{"@Type":"829a18f2","@Value":{"ParentObjects":{"@Type":"fa2ee218:IDictionary","@Value":[{"@Key":"(Guid)21af5390-2942-461a-bf89-951aaf6999f1","@Value":"(Guid)c733db44-193b-4f2a-91c4-57ad7a4c37dd"}]}}}}]},"TypeGuid":"(Guid)6f9dac99-8de1-4efc-8465-68ac443b7d08","EmbeddedTypeGuids":{"@Type":"[Guid]","@Value":["(Guid)a9ed5b7e-75c5-4651-af16-d2c27e98cb94","(Guid)3b83b776-fb25-43b8-99f2-3c507c9143fc"]}}},"TypeInfos":{"24568a24":"{24568a24-c491-472c-a21f-ee5d33859fab}","2c41fa04":"{2c41fa04-1834-41c1-816e-303c7aa2c05b}","81297157":"{81297157-7ec9-45ce-845e-84cab2b88ade}","829a18f2":"{829a18f2-c514-4f6e-9634-1df173429203}","bool":"System.Boolean","fa2ee218":"{fa2ee218-a39b-4b6d-b249-49dbddbd168a}","Guid":"System.Guid","int":"System.Int32","string":"System.String"}},"object":{"Graph":{"@Type":"6f9dac99","@Value":{"SpecialFunc":{"@Type":"0db3d7bb:Enum","@Value":"None"},"Implementation":{"@Type":"3b83b776","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)// Impuls bei Takt bilden ===============================================================================================================\nrtrigStartPulse(CLK:= In_bStart , Q=> );\n\n// Impuls als BOOLsche Variable =========================================================================================================\nbStartPulse := rtrigStartPulse.Q;\n\n// Startsignal ist erfolgt, ab jetzt wird der Takt ausgegeben ===========================================================================\nIF In_bEnable AND bStartPulse THEN\n\tbStartSignal := TRUE;  \t\t\t\t\t\t\nEND_IF\n\n// Zeitwert REAL --> TIME-Format ========================================================================================================\nIF NOT tonTimer1.IN THEN\n\ttTimeValueTimer1 := REAL_TO_TIME(In_rTimeValueClockGenerator_ms);\nEND_IF\nIF NOT tonTimer2.IN THEN\n\ttTimeValueTimer2 := REAL_TO_TIME(In_rTimeValueClockGenerator_ms);\nEND_IF\n//=======================================================================================================================================\nIF In_bEnable AND bInit THEN\n\t// Zeitglied 1 ======================================================================================================================\t\n\ttonTimer1(\tIN :=\tbTimeReachedTimer1 AND\t\n\t\t\t\t\t\t(bStartSignal OR tonTimer1.IN\t\t// Selbsthaltung\n\t\t\t\t\t\t)\n\t\t\t\t\t\tAND NOT tonTimer1.Q\t\n\t\t\t\t\t\tAND NOT tonTimer2.IN,\t\t\t// Das andere Zeitglied ist NICHT aktiv\n\t\t\t\t\t\tPT := tTimeValueTimer1\n\t\t\t  );\n\n\tbTimeReachedTimer2\t:= \t(\tNOT bTimeReachedTimer1\n\t\t\t\t\t\t\t\tAND NOT tonTimer1.Q\n\t\t\t\t\t\t\t)\n\t\t\t\t\t\t\tOR\t(\ttonTimer2.IN\n\t\t\t\t\t\t\t\t\tAND NOT tonTimer2.Q\n\t\t\t\t\t\t\t\t)\n\t\t\t\t\t\t\tOR\t(\ttonTimer1.Q\n\t\t\t\t\t\t\t\t\tAND NOT tonTimer2.Q\n\t\t\t\t\t\t\t\t\t//AND NOT FB_Zeitglied_1_TON_Takt_Takt_lokal.IN\n\t\t\t\t\t\t\t\t);\t\t\t\t\t\t\t\t\n\t\n\t// Zeitglied 2 ======================================================================================================================\t\n\ttonTimer2\t( \tIN := \tbTimeReachedTimer2\n\t\t\t\t\t\t\tAND\t(\tbStartSignal\n\t\t\t\t\t\t\t\t\tOR tonTimer2.IN\t\t// Selbsthaltung\n\t\t\t\t\t\t\t\t)\n\t\t\t\t\t\t\tAND NOT tonTimer1.IN,\t\t\t// Das andere Zeitglied ist NICHT aktiv\n\t\t\t\t\tPT := tTimeValueTimer2\n\t\t\t\t);\n\n\tbTimeReachedTimer1 :=\t(\tNOT bTimeReachedTimer2\n\t\t\t\t\t\t\t\tAND NOT tonTimer2.Q\n\t\t\t\t\t\t\t)\n\t\t\t\t\t\t\tOR\t(\ttonTimer1.IN\n\t\t\t\t\t\t\t\t\tAND NOT tonTimer1.Q\n\t\t\t\t\t\t\t\t)\n\t\t\t\t\t\t\tOR\t(\ttonTimer2.Q\n\t\t\t\t\t\t\t\t\tAND NOT tonTimer1.Q\n\t\t\t\t\t\t\t\t\t//AND NOT FB_Zeitglied_2_TON_Takt_Takt_lokal.IN\n\t\t\t\t\t\t\t\t);\n\t\t\t\t\t\t\t\t\t\t\nELSE\n\tbStartSignal := FALSE;  \t\t\t\t\t\t\n\tbTimeReachedTimer1 := TRUE;\n\tbTimeReachedTimer2 := FALSE;\t\n\ttonTimer1.IN := FALSE;\n\ttonTimer2.IN := FALSE;\n\tbInit := TRUE;\nEND_IF\n//=======================================================================================================================================\n// Ausgabe: Zeitbasierter Taktgeber\nInOut_bClock := tonTimer1.Q\tOR tonTimer2.Q;\n//=======================================================================================================================================\n","LineInfoPersistence":"(string)62ac5fa6-a7e8-4e41-a0d0-c62675174d1f_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string){attribute 'qualified_only' := ''}\n{attribute 'namespace' := 'ILGStdLib.FunctionBlocks'}\nFUNCTION_BLOCK FB_TimeBasedClockGenerator\nVAR_IN_OUT\n\t/// output: Zeitbasierter Taktgeber\n\tInOut_bClock: BOOL;\nEND_VAR\nVAR\n\t/// lokale Variable: Initialisierung ist erfolgt. Damit der Baustein sofrt nach der Initialisierung bereit ist, auch wenn bEnable_in statisch \"TRUE\" ist *)\t(* 2012-02-14 gt: hinzu *)\n\tbInit: BOOL;\n\t/// lokale Variable: Startsignal ist erfolgt, ab jetzt wird der Takt ausgegeben\n\tbStartSignal: BOOL;\n\t/// lokale Variable: Vorgegebener Zeitwert im TIME-Format: Für Zeitglied 1\n\ttTimeValueTimer1: TIME;\n\t/// lokale Variable: Vorgegebener Zeitwert im TIME-Format: Für Zeitglied 2\n\ttTimeValueTimer2: TIME;\n\t/// lokale Variable: Takt-Impuls des Startsignals bilden\n\trtrigStartPulse: R_TRIG;\n\t/// lokale Variable: Impuls: Starten des Taktgebers\n\tbStartPulse: BOOL;\n\t/// lokale Variable: Zeitglied 1 hat aktuellen Zeitwert\n\tbTimeReachedTimer1: BOOL;\n\t/// lokale Variable: Zeitglied 2 hat aktuellen Zeitwert\n\tbTimeReachedTimer2: BOOL;\n\t/// lokale Variable: Zeitglied 1 für die Taktvorgabe\n\ttonTimer1: TON;\n\t/// lokale Variable: Zeitglied 2 für die Taktvorgabe\n\ttonTimer2: TON;\nEND_VAR\nVAR_INPUT\n\t/// input: Solange der \"bEnable\" nicht \"TRUE\" ist, erfolgt keine Auswertung\n\tIn_bEnable: BOOL;\n\t/// input: Start des zeitbasierten Taktgebers\n\tIn_bStart: BOOL;\n\t/// [ms] Zeitwert für den Taktgeber\n\tIn_rTimeValueClockGenerator_ms: REAL;\nEND_VAR\n","LineInfoPersistence":"(string)62ac5fa6-a7e8-4e41-a0d0-c62675174d1f_Decl_LineIds"}}}},"UniqueIdGenerator":"(string)111","POULevel":{"@Type":"8e575c5b:Enum","@Value":"Standard"},"ChildObjectGuids":{"@Type":"ArrayList:IList","@Value":[]},"AddAttributeSubsequent":"(bool)False"}},"TypeInfos":{"0db3d7bb":"{0db3d7bb-cde0-4416-9a7b-ce49a0124323}","3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","6f9dac99":"{6f9dac99-8de1-4efc-8465-68ac443b7d08}","8e575c5b":"{8e575c5b-1d37-49c6-941b-5c0ec7874787}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","ArrayList":"System.Collections.ArrayList","bool":"System.Boolean","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}