igkeit der anparametrierten\t     ## *)\n(* ##\tZeitdauer.\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t ## *)\n(* ##\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t ## *)\n(* ##   Versions-History:\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t ## *)\n(* ##   06. Dezember 2011\t\tVersion 1.0\t\t-\terste Version.\t\t\t\t\t\t\t\t\t\t\t\t\t ## *)\n(* ##\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t ## *)\n(* ##\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t ## *)\n(* #################################################################################################################### *)\n\n\n(* -------------------------------------------------------------------------------------------------------------------- *)\n(* -- Erzeugen des Taktsignals ---------------------------------------------------------------------------------------- *)\t\n(* -------------------------------------------------------------------------------------------------------------------- *)\n\nIF In_bEnable THEN\n\n\tfbTonClock1(IN:= (NOT Out_bClock), PT:= In_tCycleTime, Q=> , ET=> );\t\t\t\t\t(* Aufruf der Einschaltverzoegerung 1 *)\n\t\t\n\tfbTonClock2(IN:= (Out_bClock), PT:= In_tCycleTime, Q=> , ET=> );\t\t\t\t\t\t(* Aufruf der Einschaltverzoegerung 2 *)\n\t\n\tIF fbTonClock1.Q THEN\n\t\tOut_bClock := TRUE;\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t(* Ausgang setzen *)\n\tELSIF fbTonClock2.Q THEN\n\t\tOut_bClock := FALSE;\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t(* Ausgang zurueck setzen *)\n\tEND_IF\n\t\n\t(* Impuls von Takt bilden *)\n\trtrig_Clock(CLK:= Out_bClock);\n\tOut_bPulse := rtrig_Clock.Q;\n\t\nELSE\n\tOut_bClock\t\t:= FALSE;\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t(* Ausgang abloeschen *)\n\tfbTonClock1(IN:= FALSE, PT:= In_tCycleTime, Q=> , ET=> );\t\t\t\t\t\t\t(* Funktionsbaustein zurueck setzen *)\n\tfbTonClock2(IN:= FALSE, PT:= In_tCycleTime, Q=> , ET=> );\t\t\t\t\t\t\t(* Funktionsbaustein zurueck setzen *)\nEND_IF\n","LineInfoPersistence":"(string)c541e665-7269-41e7-92c9-f2fa2f99cd2e_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string){attribute 'qualified_only' := ''}\n{attribute 'namespace' := 'ILGStdLib.FunctionBlocks'}\nFUNCTION_BLOCK FB_ClockMarker\nVAR_OUTPUT\n\t/// Ausgangsvariable, die in der anparametrierten rRDM_Taktzeit_gb taktet \n\tOut_bClock: BOOL;\n\t/// Ausgangsvariable, die in der anparametrierten rRDM_Taktzeit_gb einen Impuls abgibt \n\tOut_bPulse: BOOL;\nEND_VAR\nVAR\n\t/// Funktionsbaustein \"Einschaltverzoegerung\" fuer den Taktmerker \n\tfbTonClock1: TON;\n\t/// Funktionsbaustein \"Einschaltverzoegerung\" fuer den Taktmerker \n\tfbTonClock2: TON;\n\t/// Funktionsbaustein positive Flanke für Taktmerker \n\trtrig_Clock: R_TRIG;\nEND_VAR\nVAR_INPUT\n\t/// Ansteuersignal fuer den Start des Funktionsbausteins / das Erzeugen das Taktimpulses \n\tIn_bEnable: BOOL;\n\t/// Anzuparametrierende Zeit, fuer die rRDM_Taktzeit_gb \n\tIn_tCycleTime: TIME;\nEND_VAR\n","LineInfoPersistence":"(string)c541e665-7269-41e7-92c9-f2fa2f99cd2e_Decl_LineIds"}}}},"UniqueIdGenerator":"(string)63","POULevel":{"@Type":"8e575c5b:Enum","@Value":"Standard"},"ChildObjectGuids":{"@Type":"ArrayList:IList","@Value":[]},"AddAttributeSubsequent":"(bool)False"}},"TypeInfos":{"0db3d7bb":"{0db3d7bb-cde0-4416-9a7b-ce49a0124323}","3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","6f9dac99":"{6f9dac99-8de1-4efc-8465-68ac443b7d08}","8e575c5b":"{8e575c5b-1d37-49c6-941b-5c0ec7874787}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","ArrayList":"System.Collections.ArrayList","bool":"System.Boolean","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}