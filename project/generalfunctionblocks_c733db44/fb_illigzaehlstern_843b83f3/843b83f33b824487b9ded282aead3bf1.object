{"payload":{"meta":{"Graph":{"@Type":"81297157","@Value":{"Guid":"(Guid)843b83f3-3b82-4487-b9de-d282aead3bf1","ParentGuid":"(Guid)00000000-0000-0000-0000-000000000000","Name":"(string)FB_IlligZaehlstern","Properties":{"@Type":"2c41fa04:IDictionary","@Value":[{"@Key":"(Guid)24568a24-c491-472c-a21f-ee5d33859fab","@Value":{"@Type":"24568a24","@Value":{"MemoryReserveForOnlineChange":"(int)0","ExcludeFromBuild":"(bool)False","External":"(bool)False","EnableSystemCall":"(bool)False","CompilerDefines":"(string)","LinkAlways":"(bool)False","Undefines":{"@Type":"[string]","@Value":[]}}}},{"@Key":"(Guid)829a18f2-c514-4f6e-9634-1df173429203","@Value":{"@Type":"829a18f2","@Value":{"ParentObjects":{"@Type":"fa2ee218:IDictionary","@Value":[{"@Key":"(Guid)21af5390-2942-461a-bf89-951aaf6999f1","@Value":"(Guid)c733db44-193b-4f2a-91c4-57ad7a4c37dd"}]}}}}]},"TypeGuid":"(Guid)6f9dac99-8de1-4efc-8465-68ac443b7d08","EmbeddedTypeGuids":{"@Type":"[Guid]","@Value":["(Guid)a9ed5b7e-75c5-4651-af16-d2c27e98cb94","(Guid)3b83b776-fb25-43b8-99f2-3c507c9143fc"]}}},"TypeInfos":{"24568a24":"{24568a24-c491-472c-a21f-ee5d33859fab}","2c41fa04":"{2c41fa04-1834-41c1-816e-303c7aa2c05b}","81297157":"{81297157-7ec9-45ce-845e-84cab2b88ade}","829a18f2":"{829a18f2-c514-4f6e-9634-1df173429203}","bool":"System.Boolean","fa2ee218":"{fa2ee218-a39b-4b6d-b249-49dbddbd168a}","Guid":"System.Guid","int":"System.Int32","string":"System.String"}},"object":{"Graph":{"@Type":"6f9dac99","@Value":{"SpecialFunc":{"@Type":"0db3d7bb:Enum","@Value":"None"},"Implementation":{"@Type":"3b83b776","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)(*******************************************************************************************************************************************)\n(* Ansteuerungen verzögern *)\n(*******************************************************************************************************************************************)\n\t(* positive Richtung *)\n\t\ttofCtrlPos (IN := In_bCtrlPos, PT:= REAL_TO_TIME(In_rNominalDelayEvaluation));\n\t\t\n\t(* Negative Richtung *)\n\t\ttofCtrlNeg (IN := In_bCtrlNeg, PT:= REAL_TO_TIME(In_rNominalDelayEvaluation));\n\t\n\t(* Achse wird angesteuert *)\n\t\tIF tofCtrlPos.Q OR tofCtrlNeg.Q THEN\n\t\t\tbCtrl:=TRUE;\t\n\t\tELSE\n\t\t\tbCtrl:=FALSE;\n\t\tEND_IF\t\n\t\t\t\n(*******************************************************************************************************************************************)\n(* Flanke bilden *)\n(*******************************************************************************************************************************************)\n \t(* positive Flankenauswertung Zählstern *)\t\n\t\trtrigZaehlstern(CLK:= In_bInput);\n\t(* Negative Flankenauswertung Zählstern *)\t\n\t\tftrigZaehlstern(CLK:= In_bInput);\n(*******************************************************************************************************************************************)\n(* Position bilden *)\n(*******************************************************************************************************************************************)\n\t(* Zählstern auswerten *)\n\t\tIF In_bEnable THEN\n\t\t\t(*Positive Richtung*)\n\t\t\t\tIF tofCtrlPos.Q AND rtrigZaehlstern.Q THEN\n\t\t\t\t\tInOut_rPosition:=InOut_rPosition + In_rNominalPositiveEdge;\n\t\t\t\tEND_IF\n\t\t\t(*Negative Richtung*)\n\t\t\t\tIF tofCtrlNeg.Q AND ftrigZaehlstern.Q THEN\n\t\t\t\t\tInOut_rPosition:=InOut_rPosition - In_rNominalPositiveEdge;\n\t\t\t\tEND_IF\n\t\tEND_IF\n\t\n(*******************************************************************************************************************************************)\n(* Fehlerauswertung *)\n(*******************************************************************************************************************************************)\n\t(* Fehler Blockierüberwachung *)\n\t(* Achse wird angesteuert und es findet kein Flankenwechsel des Zählsterns statt *)\t\n\t\n\t\tIF (In_bBlockingMonitoringAsLongAsActivation = TRUE) THEN\n\t\t\t(* 2014-01-08 Carle: geändert STAL 90 *)(* (bCtrl_pos_i OR bCtrl_neg_i) statt bCtrl *)\n\t\t\ttonBlockingMonitoring(IN := (In_bCtrlPos OR In_bCtrlNeg) AND NOT rtrigZaehlstern.Q AND NOT ftrigZaehlstern.Q,\tPT:= REAL_TO_TIME(In_NominalBlockingDelay));\t\t\n\t\t\tIF tonBlockingMonitoring.Q THEN\n\t\t\t\tOut_bErrorBlockingMonitoring:= TRUE;\n\t\t\tELSE\n\t\t\t\tOut_bErrorBlockingMonitoring:= FALSE;\n\t\t\tEND_IF\n\t\tELSE\n\t\t\ttonBlockingMonitoring(IN := bCtrl AND\tNOT rtrigZaehlstern.Q AND\tNOT ftrigZaehlstern.Q, PT:= REAL_TO_TIME(In_NominalBlockingDelay));\n\t\t\n\t\t\tIF tonBlockingMonitoring.Q THEN\n\t\t\t\tOut_bErrorBlockingMonitoring:= TRUE;\n\t\t\tELSE\n\t\t\t\tOut_bErrorBlockingMonitoring:= FALSE;\n\t\t\tEND_IF\n\t\tEND_IF","LineInfoPersistence":"(string)843b83f3-3b82-4487-b9de-d282aead3bf1_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string){attribute 'qualified_only' := ''}\n{attribute 'namespace' := 'ILGStdLib.FunctionBlocks'}\nFUNCTION_BLOCK FB_IlligZaehlstern\nVAR_OUTPUT\n\t/// Fehler Blockierüberwachung \n\tOut_bErrorBlockingMonitoring: BOOL;\nEND_VAR\nVAR_IN_OUT\n\t/// ISTALosition [mm] \n\tInOut_rPosition: REAL;\nEND_VAR\nVAR\n\t/// interne Variable: Ansteuerung der Achse aktiv \n\tbCtrl: BOOL;\n\t/// interne Variable: Schrittnummer für Schrittkette \"Referenzieren mit Referenzschalter\" \n\tiStepNo: INT;\n\t/// interne Variable: Steigende Flanke Zählstern \n\trtrigZaehlstern: R_TRIG;\n\t/// interne Variable: Fallende Flanke Zählstern \n\tftrigZaehlstern: R_TRIG;\n\t/// interne Variable: Zeitverzögerung Ansteuerung positiv \n\ttofCtrlPos: TOF;\n\t/// interne Variable: Zeitverzögerung Ansteuerung negativ \n\ttofCtrlNeg: TOF;\n\t/// interne Variable: Zeitüberwachung Referenzieren \n\ttonBlockingMonitoring: TON;\nEND_VAR\nVAR_INPUT\n\t/// Achse wird positiv angesteuert \n\tIn_bCtrlPos: BOOL;\n\t/// Achse wird positiv angesteuert \n\tIn_bCtrlNeg: BOOL;\n\t/// Zählstern auswerten \n\tIn_bEnable: BOOL;\n\t/// Eingang des Zählsterns \n\tIn_bInput: BOOL;\n\t///Sollwert Position pro Zählstern-Flanke [mm] \n\tIn_rNominalPositiveEdge: REAL;\n\t///Sollwert Verzögerung Zählstern auswerten [ms] \n\tIn_rNominalDelayEvaluation: REAL;\n\t///Sollwert Verzögerung Blockierüberwachung [ms] \n\tIn_NominalBlockingDelay: REAL;\n\t/// Blockierüberwachung nur so lange wie Ansteuerung RDM=FALSE, STAL=TRUE \n\tIn_bBlockingMonitoringAsLongAsActivation: BOOL := FALSE;\nEND_VAR\n","LineInfoPersistence":"(string)843b83f3-3b82-4487-b9de-d282aead3bf1_Decl_LineIds"}}}},"UniqueIdGenerator":"(string)107","POULevel":{"@Type":"8e575c5b:Enum","@Value":"Standard"},"ChildObjectGuids":{"@Type":"ArrayList:IList","@Value":[]},"AddAttributeSubsequent":"(bool)False"}},"TypeInfos":{"0db3d7bb":"{0db3d7bb-cde0-4416-9a7b-ce49a0124323}","3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","6f9dac99":"{6f9dac99-8de1-4efc-8465-68ac443b7d08}","8e575c5b":"{8e575c5b-1d37-49c6-941b-5c0ec7874787}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","ArrayList":"System.Collections.ArrayList","bool":"System.Boolean","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}