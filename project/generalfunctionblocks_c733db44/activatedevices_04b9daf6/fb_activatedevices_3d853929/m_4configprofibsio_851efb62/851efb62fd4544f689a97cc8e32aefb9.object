t\tSlaveDeactAddr = 30\n\t\tSlaveDeactAddr = 112\n\t\tSlaveDeactAddr = 3\n\t*)\n\n\t\t//\tHeader\n\n\t\tasStringCmdCfg[iLineCfg] := ';--------------- PROFINET IO --------------- ';\n\t\tiLineCfg := iLineCfg + 1;\t \t// next line\n\t\t\n\t\tFOR j := 1 TO iMAX_PROFIBUS_MASTER DO\n//\t\t\tIF ProfibusIOCmdConfig[j,0].Active THEN\t\t\t// Master is active\n\t\t\t\tasStringCmdCfg[iLineCfg] := '[ProfibusDP.Master.';\n\t\t\t\tsTemp2 := INT_TO_STRING(j-1);\n\t\t\t\t//strConcat(ADR\t(arStringCmdCfg[iLineCfg]), SIZEOF(arStringCmdCfg[iLineCfg]) ,\tADR(strTemp));\n\t\t\t\tIL_CONCAT(ADR(asStringCmdCfg[iLineCfg]), ADR(sTemp2), ADR(asStringCmdCfg[iLineCfg]), SIZEOF(asStringCmdCfg[iLineCfg]));\n\t\t\t\tsTemp2 := ']';\n\t\t\t\t//strConcat(ADR\t(arStringCmdCfg[iLineCfg]), SIZEOF(arStringCmdCfg[iLineCfg]) ,\tADR(strTemp));\n\t\t\t\tIL_CONCAT(ADR(asStringCmdCfg[iLineCfg]), ADR(sTemp2), ADR(asStringCmdCfg[iLineCfg]), SIZEOF(asStringCmdCfg[iLineCfg]));\n\t\t\t\t\n\t\t\t\tiLineCfg := iLineCfg +1;\t \t// next line\n\t\n\t\t\t\tFOR i := 1 TO iMAX_PROFIBUS_SLAVE DO\n\t\t\t\t\tIF refastProfibusIOCmdConfig[j,i].iAddress <> 0 THEN \n\t\t\t\t\t\tIF refastProfibusIOCmdConfig[j,i].bActive THEN\n\t\t\t\t\t\t\tasStringCmdCfg[iLineCfg] := '; ';\n\t\t\t\t\t\tELSE\t\t\t\n\t\t\t\t\t\t\tasStringCmdCfg[iLineCfg] := '';\n\t\t\t\t\t\tEND_IF\t\n\t\t\t\t\t\t\n\t\t\t\t\t\tsTemp2 := 'SlaveDeactAddr = ';\n\t\t\t\t\t\t//strConcat(ADR\t(arStringCmdCfg[iLineCfg]), SIZEOF(arStringCmdCfg[iLineCfg]) ,\tADR(strTemp));\n\t\t\t\t\t\tIL_CONCAT(ADR(asStringCmdCfg[iLineCfg]), ADR(sTemp2), ADR(asStringCmdCfg[iLineCfg]), SIZEOF(asStringCmdCfg[iLineCfg]));\n\t\t\t\t\t\tsTemp2 := INT_TO_STRING (refastProfibusIOCmdConfig[j,i].iAddress);\n\t\t\t\t\t\t//strConcat(ADR\t(arStringCmdCfg[iLineCfg]), SIZEOF(arStringCmdCfg[iLineCfg]) ,\tADR(strTemp));\n\t\t\t\t\t\tIL_CONCAT(ADR(asStringCmdCfg[iLineCfg]), ADR(sTemp2), ADR(asStringCmdCfg[iLineCfg]), SIZEOF(asStringCmdCfg[iLineCfg]));\n\t\t\t\t\t\t\n\t\t\t\t\t\tIF NOT (asStringCmdCfg[iLineCfg]= asStringActCfg[iLineCfg]) THEN\n\t\t\t\t\t\t\tOUT_bReboot := TRUE;\n\t\t\t\t\t\tEND_IF\n\t\t\t\t\t\n\t\t\t\t\t\tiLineCfg := iLineCfg +1; \t// next line\n\t\t\t\t\t\n\t\t\t\t\tELSIF refastProfibusIOCmdConfig[j,i].iAddress = 0 THEN\n\t\t\t\t\t\t// End LOOP\n\t \t\t\t\t\ti := iMAX_PROFIBUS_SLAVE + 1;\t\n\t\t\t\t\tEND_IF\t\n\t\t\t\t\t\n\t\t\t\t\n\t\t\t\tEND_FOR\n\t\t\t\t\n\t\t\t\tasStringCmdCfg[iLineCfg] := ';-------------------------------------------'\t;\n\t\t\t\tiLineCfg := iLineCfg +1;\t \t// next line\n\t\t\n\t\t\t\tasStringCmdCfg[iLineCfg] := ''\t;\n\t\t\t\tiLineCfg := iLineCfg +1;\t \t// next line\n\t\t\t\t\n\t\t\t\tiEOF := iLineCfg -1 ;\t\t\t//End Of File\n//\t\t\tEND_IF\t\n\t\tEND_FOR\t\n\t\tiState := 499;\n\t\t\n(* ################################################################################ *)\t\n\t// End\n(* ################################################################################ *)\t\t\t\t\t\n\t499:\n\t\tiState := 500;\t\t\n\nEND_CASE","LineInfoPersistence":"(string)851efb62-fd45-44f6-89a9-7cc8e32aefb9_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)METHOD M_4ConfigProfibsIO : BOOL\n","LineInfoPersistence":"(string)851efb62-fd45-44f6-89a9-7cc8e32aefb9_Decl_LineIds"}}}}}},"TypeInfos":{"3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","f8a58466":"{f8a58466-d7f6-439f-bbb8-d4600e41d099}","string":"System.String"}}},"FormatVersion":"1.0"}